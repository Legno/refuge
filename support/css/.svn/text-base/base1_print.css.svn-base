}


table.aabb {
border-collapse:  collapse;
border: 1px solid #ff6600;
/* border: solid 2px #666666;*/
 }

 table.aabb * {
border-collapse:  collapse;
border: 1px solid #ff6600;
 }

table.aabb th {
background-color: orange;
border-collapse:  collapse;
border: 1px solid #ff6600;
 }


table#bbb  {
	border-collapse: collapse;
}
table#bbb td  {
	border:thin solid green;
}
table#bbb th  {
	border:thin solid blue;
	background-color: yellow;

}
a {
	text-decoration: none;
}

